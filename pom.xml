    </parent>
    <groupId>org.lanqiao</groupId>
    <artifactId>read</artifactId>
    <version>0.0.1-SNAPSHOT</version>


    <name>read</name>
    <description>Demo project for Spring Boot</description>

    <properties>
        <java.version>1.8</java.version>
    </properties>

